root = true

[*]
charset = utf-8
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.cs]
indent_style = space
indent_size = 4
csharp_new_line_before_open_brace = all
dotnet_diagnostic.IDE0060.severity = suggestion   # unused params
dotnet_diagnostic.IDE0018.severity = suggestion   # inline vars
